.swiper{
  width: 750rpx;
  height: 340rpx;
}
.swiper image{
  width: 100%;
}
.nav{
  display: flex;
}
.nav .nav-a{
  flex: 1;
  padding: 20rpx;
}
.nav .nav-a image{
  width: 100%;
}
.floor-title{
width: 100%;
margin-top: 20rpx;
}
.floor-content .floor-content-navigate{
  float: left;
  width: 33.33%;
}
.floor-content .floor-content-navigate:nth-last-child(-n+4){
  height: calc( 33.33vw * 386 /  232 /2);
  border-left: 10rpx solid #fff;
}
.floor-content .floor-content-navigate image{
  width: 100%;
  height: 100%;
  
}
.floor-content .floor-content-navigate:nth-child(2),
.floor-content .floor-content-navigate:nth-child(3){
border-bottom: 10rpx solid #fff;
}